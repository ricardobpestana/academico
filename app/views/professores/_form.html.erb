
<ul>
	<% @professor.errors.full_messages.each do |msg| %>
		<li><%= msg %> </li>
	<% end %>
</ul>

<br>

<%= form_for @professor do |f| %>
	Matrícula: <%= f.text_field :matricula %> <br/>
	Nome: <%= f.text_field :nome %> <br/>
	E-mail: <%= f.text_field :email %> <br/>
	Carga Horária Semanal: <%= f.text_field :ch_semanal %> <br/>
	Data de Admissão: <%= f.text_field :data_adm %> <br/>
	<%= f.submit "Salvar" %>
<% end %>


